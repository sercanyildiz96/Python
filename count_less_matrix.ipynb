   "source": [
    "def count_less_than(matrix, target):\n",
    "    coun = 0\n",
    "    for i in matrix:\n",
    "        for j in range(len(i)-1, -1, -1):\n",
    "            if i[j] < target:\n",
    "                coun += j + 1\n",
    "                break\n",
    "    return coun"
   ]
