   "source": [
    "def next_prime(n):\n",
    "    if n < 2:\n",
    "        return 2\n",
    "    while True:\n",
    "        n = n + 1\n",
    "        flag = 0\n",
    "        for i in range(2, n):\n",
    "            if n % i == 0:\n",
    "                flag = 1\n",
    "                break\n",
    "        if flag == 0:\n",
    "            return n"
   ]
